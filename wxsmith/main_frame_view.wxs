<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="MainFrameView">
		<title>Moarload</title>
		<size>640,480</size>
		<id_arg>0</id_arg>
		<object class="wxPanel" name="ID_MAIN_FRAME_PANEL" variable="mainFramePanel" member="yes">
			<pos>80,56</pos>
			<object class="wxBoxSizer" variable="mainFrameSizer" member="no">
				<orient>wxVERTICAL</orient>
				<object class="sizeritem">
					<object class="wxNotebook" name="ID_MAIN_NOTEBOOK" variable="mainNotebook" member="yes">
						<object class="notebookpage">
							<object class="wxPanel" name="ID_DOWNLOAD_PANEL" variable="downloadPanel" member="yes">
								<object class="wxBoxSizer" variable="BoxSizer1" member="no">
									<object class="sizeritem">
										<object class="wxSplitterWindow" name="ID_SPLITTERWINDOW1" variable="SplitterWindow1" member="yes">
											<minsize>10</minsize>
											<orientation>vertical</orientation>
											<minsize>10,10</minsize>
											<object class="wxPanel" name="ID_FILTER_PANEL" variable="filterPanel" member="yes">
												<object class="wxBoxSizer" variable="BoxSizer2" member="no">
													<object class="sizeritem">
														<object class="wxListBox" name="ID_LISTBOX1" variable="ListBox1" member="yes">
															<content>
																<item>All</item>
																<item>Downloading</item>
																<item>Completed</item>
																<item>Paused</item>
															</content>
															<default>-1</default>
															<style>wxLB_SINGLE</style>
														</object>
														<flag>wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
														<option>1</option>
													</object>
												</object>
											</object>
											<object class="wxPanel" name="ID_DOWNLOAD_LIST_PANEL" variable="downloadListPanel" member="yes">
												<object class="wxBoxSizer" variable="BoxSizer3" member="no" />
											</object>
										</object>
										<flag>wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
										<option>1</option>
									</object>
								</object>
							</object>
							<label>Downloads</label>
						</object>
					</object>
					<flag>wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
					<border>5</border>
					<option>1</option>
				</object>
				<object class="sizeritem">
					<object class="wxPanel" name="ID_MAIN_STATUS_PANEL" variable="mainStatusPanel" member="yes">
						<minsize>-1,24</minsize>
						<maxsize>-1,24</maxsize>
						<object class="wxBoxSizer" variable="mainStatusPanelSizer" member="no">
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
									<label>Label</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
									<label>Label</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
									<label>Label</label>
								</object>
								<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
					</object>
					<flag>wxEXPAND|wxALIGN_BOTTOM|wxALIGN_CENTER_HORIZONTAL</flag>
				</object>
			</object>
		</object>
		<object class="wxMenuBar" variable="mainMenuBar" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxToolBar" name="ID_MAIN_TOOLBAR" variable="mainToolBar" member="yes">
			<style>wxTB_HORIZONTAL|wxTB_TEXT|wxNO_BORDER</style>
			<object class="tool" name="ID_TOOLBARITEM1" variable="ToolBarItem1" member="yes">
				<label>Start/Stop</label>
				<bitmap stock_id="wxART_GO_FORWARD" />
			</object>
			<object class="separator" />
			<object class="tool" name="ID_TOOLBARITEM3" variable="ToolBarItem2" member="yes">
				<label>Stop Next</label>
				<bitmap stock_id="wxART_GO_DOWN" />
			</object>
			<object class="tool" name="ID_TOOLBARITEM4" variable="ToolBarItem3" member="yes">
				<label>Auto Reconnect</label>
				<bitmap stock_id="wxART_ERROR" />
			</object>
			<object class="tool" name="ID_TOOLBARITEM5" variable="ToolBarItem4" member="yes">
				<label>Auto Clipboard</label>
				<bitmap stock_id="wxART_PASTE" />
			</object>
			<object class="separator" />
			<object class="tool" name="ID_TOOLBARITEM6" variable="ToolBarItem5" member="yes">
				<label>Reconnect Now</label>
				<bitmap stock_id="wxART_ERROR" />
			</object>
		</object>
	</object>
</wxsmith>
